{
  "name": "adonis-lucid-filter",
  "version": "2.1.0-rc",
  "description": "Addon for filtering AdonisJS Lucid ORM",
  "main": "./build/providers/LucidFilterProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/commands",
    "build/templates",
    "build/src"
  ],
  "scripts": {
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "rm -rf build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"templates/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-lucid-filter.git"
  },
  "keywords": [
    "adonisjs",
    "adonis-lucid",
    "adonis-filter",
    "adonis-lucid-filter"
  ],
  "author": "LookinGit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-lucid-filter/issues"
  },
  "devDependencies": {
    "@adonisjs/ace": "^8.0.0",
    "@adonisjs/application": "^3.0.21",
    "@adonisjs/config": "^2.0.3",
    "@adonisjs/core": "^5.0.4-preview-rc-2.1",
    "@adonisjs/events": "^5.0.6",
    "@adonisjs/fold": "^7.0.9",
    "@adonisjs/logger": "^3.0.4",
    "@adonisjs/lucid": "^10.0.0",
    "@adonisjs/mrm-preset": "^2.4.0",
    "@adonisjs/profiler": "^5.0.4",
    "@poppinss/dev-utils": "^1.0.11",
    "@types/node": "^14.14.14",
    "copyfiles": "^2.4.1",
    "eslint": "^7.15.0",
    "eslint-plugin-adonis": "^1.0.15",
    "japa": "^3.1.1",
    "knex": "^0.21.13",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "directories": {
    "test": "test"
  },
  "adonisjs": {
    "types": "adonis-lucid-filter",
    "providers": [
      "adonis-lucid-filter"
    ],
    "commands": [
      "adonis-lucid-filter/build/commands"
    ]
  }
}
