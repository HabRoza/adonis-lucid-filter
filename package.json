{
  "name": "adonis-lucid-filter",
  "version": "2.0.0-rc4",
  "description": "Addon for filtering AdonisJS Lucid ORM",
  "main": "./build/providers/LucidFilterProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/commands",
    "build/templates",
    "build/src"
  ],
  "scripts": {
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "rm -rf build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"templates/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-lucid-filter.git"
  },
  "keywords": [
    "adonisjs",
    "adonis-lucid",
    "adonis-filter",
    "adonis-lucid-filter"
  ],
  "author": "LookinGit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-lucid-filter/issues"
  },
  "devDependencies": {
    "@adonisjs/ace": "^6.9.2",
    "@adonisjs/application": "^1.3.13",
    "@adonisjs/config": "^1.0.14",
    "@adonisjs/events": "^2.0.1",
    "@adonisjs/fold": "^6.3.5",
    "@adonisjs/logger": "^2.0.4",
    "@adonisjs/lucid": "^8.1.0",
    "@adonisjs/mrm-preset": "^2.3.0",
    "@adonisjs/profiler": "^3.0.1",
    "@poppinss/dev-utils": "^1.0.6",
    "@types/node": "^14.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.8",
    "japa": "^3.0.1",
    "knex": "^0.21.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "directories": {
    "test": "test"
  },
  "adonisjs": {
    "types": "adonis-lucid-filter",
    "providers": [
      "adonis-lucid-filter"
    ],
    "commands": [
      "adonis-lucid-filter/build/commands"
    ]
  }
}
